<?php
namespace arithmeticMath;

class ArithmeticMath
{
  
    // method declaration
    public function add($var1, $var2)
    {
        return $var1+$var2;
    }

    public function subtract($var1, $var2)
    {
        return $var1-$var2;
    }

    public function multiply($var1, $var2)
    {
        return $var1*$var2;
    }

    public function divide($var1, $var2)
    {
        return $var1/$var2;
    }
}
