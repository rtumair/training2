<?php
namespace foo;

use PHPUnit_Framework_TestCase as TestCase;
include_once 'Arithmetic.php';

class ArithmeticTest extends TestCase
{
    public function testSum()
    {
     $obj = new Arithmetic;
     $this->assertEquals(3,$obj->add(2, 1));
    }
    public function testSub()
    {
     $obj = new Arithmetic;
     $this->assertEquals(2,$obj->subtract(2, 1));
    }
    public function testMul()
    {
     $obj = new Arithmetic;
    $this->assertEquals(2,$obj->multiply(2, 1));
    }
    public function testDiv()
    {
     $obj = new Arithmetic;
     $this->assertEquals(2,$obj->divide(2, 1));

    }
}


